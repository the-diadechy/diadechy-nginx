{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "rootDir":"./data/ts",
    "outDir": "./data/js"
  }
}
